{
  "name": "emma.css",
  "version": "0.8.0",
  "description": "Emma.css { emmet-like utility classes }",
  "scripts": {
    "build": "npm run compile",
    "compile:json": "js-yaml src/emma.yml > tmp/emma.json",
    "compile:ts": "tsc",
    "compile:js": "node tmp/index.js",
    "compile:sass": "node-sass --output-style compact emma.scss emma.css",
    "compile": "npm run compile:json && npm run compile:ts && npm run compile:js && npm run compile:sass && npm run remove-empty-lines",
    "remove-empty-lines": "gulp remove-empty-lines",
    "watch": "watch 'npm run test' src/ test/ --wait 10 -d -u",
    "test": "npm run compile:json && mocha"
  },
  "author": "ruedap",
  "license": "MIT",
  "main": "emma.css",
  "style": "emma.css",
  "scss": "emma.scss",
  "files": [
    "sass",
    "CHANGELOG.md",
    "emma.css",
    "emma.scss",
    "LICENSE",
    "README.md"
  ],
  "homepage": "https://github.com/ruedap/emma.css",
  "keywords": [
    "sass",
    "scss",
    "css",
    "emmet",
    "utility",
    "coding"
  ],
  "bugs": {
    "url": "https://github.com/ruedap/emma.scss/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ruedap/emma.css.git"
  },
  "devDependencies": {
    "@types/fs-extra": "^3.0.1",
    "@types/js-yaml": "3.5",
    "@types/lodash": "^4.14.64",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.18",
    "@types/power-assert": "1.4",
    "@types/sinon": "2.2",
    "babel": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "espower-typescript": "^8.0.0",
    "fs-extra": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-postcss": "^7.0.0",
    "gulp-remove-empty-lines": "^0.0.8",
    "js-yaml": "3.5",
    "lodash": "^4.17.4",
    "mocha": "^3.4.1",
    "node-sass": "^4.5.2",
    "postcss": "^6.0.1",
    "postcss-map": "^0.9.0",
    "power-assert": "1.4",
    "sinon": "2.2",
    "typescript": "^2.3.2",
    "watch": "^1.0.2"
  }
}
